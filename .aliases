# Aliases for 'ls' command variations
alias la='ls -lAh'
alias ll='ls -lah'

# Aliases for directory navigation
alias up='cd ..'
alias up2='cd ../..'
alias up3='cd ../../..'
alias up4='cd ../../../..'

# Aliases for frequent commands
alias diff='diff --color=auto'
alias mkdir='mkdir -pv'
alias vi='vim'

# Kubernetes shortcut
alias k=kubectl

# GitHub Copilot CLI
alias cs=ghcs
alias ce=ghce
