[alias]
    br = branch
    ci = commit
    co = checkout
    st = status
	pushalltags = !git push origin --tags && git push backup --tags
	pushall = !git push origin --all && git push backup --all
[core]
    editor = vim
    autocrlf = input
[init]
    defaultBranch = main
[color]
    ui = true
[include]
    path = ~/.git-private
[pull]
	rebase = false
