[alias]
    br = branch
    ci = commit
    co = checkout
    st = status
    cam = !git add -A && git commit -m
	pushalltags = !git push origin --tags && git push backup --tags
	pushall = !git push origin --all && git push backup --all
    undo = reset --soft HEAD^
[core]
    editor = vim
    autocrlf = input
[init]
    defaultBranch = main
[color]
    ui = true
[include]
    path = $HOME/.git-private
[pull]
	rebase = false
